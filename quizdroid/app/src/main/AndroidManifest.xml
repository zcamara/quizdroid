<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="zcamara.washington.edu.quizdroid" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".QuestionOverview"
            android:label="@string/title_activity_question_overview" >
        </activity>
        <activity
            android:name=".QuestionPage"
            android:label="@string/title_activity_question_page" >
        </activity>
        <activity
            android:name=".QuestionResults"
            android:label="@string/title_activity_question_results" >
        </activity>
    </application>

</manifest>
